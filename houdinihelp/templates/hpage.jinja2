{% extends "/templates/page.jinja2" %}

{% block styles %}
    {{ super() }}
    <link rel="stylesheet" href="{{ rel("/static/css/hpage.css") }}">
    <!-- {{ config.VARS.MAJOR_VERSION }}.{{ config.VARS.MINOR_VERSION }}.{{ config.VARS.BUILD_VERSION }} -->
{% endblock %}

{% block scripts %}
    {{ super() }}
    <script type="text/javascript" src="{{ rel("/static/js/load.js") }}"></script>
{% endblock %}

{% block brand -%}
    <a href="{{ rel("/") }}" class="brand navitem"><img src="{{ rel("/icons/MISC/logo.svg") }}" width="32" height="32" /></a>
{%- endblock %}

{% block searchbox %}
    {% if config.VARS.USE_GOOGLE %}
        <div id="googlebox" class="navitem">
            <form action="http://www.google.com/cse" id="cse-search-box">
		        <input type="hidden" name="cx" value="001106583893786776783:w98mmkqk_7e" />
                <input type="hidden" name="ie" value="UTF-8" />
                <input type="text" name="q" size="31" />
                <input type="submit" name="sa" value="Search" />
            </form>
        </div>
        <script type="text/javascript" src="http://www.google.com/coop/cse/brand?form=cse-search-box&amp;lang=en">
        // This comment prevents the wiki2html translator from collapsing the opening and closing 'script' tags into one.  Collapsed script tags do not work on all browsers.
        </script>
    {% else %}
        {{ super() }}
    {% endif %}
{% endblock %}
